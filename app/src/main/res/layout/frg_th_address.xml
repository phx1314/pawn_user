<?xml version="1.0" encoding="UTF-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#fff"
    android:gravity="center_horizontal"
    android:orientation="vertical">

    <ListView
        android:id="@+id/mAbPullListView"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/mTextView_yd"
        android:layout_weight="1"
        android:cacheColorHint="#00000000"
        android:divider="@null"
        android:footerDividersEnabled="true"
        android:headerDividersEnabled="false"
        android:listSelector="#00000000" />

    <TextView
        android:id="@+id/mImageView_add"
        android:layout_width="match_parent"
        android:layout_height="@dimen/j45dp"
        android:layout_marginLeft="@dimen/j25dp"
        android:layout_marginTop="@dimen/j30dp"
        android:layout_marginBottom="@dimen/j30dp"
        android:layout_marginRight="@dimen/j25dp"
        android:background="@drawable/shape_login"
        android:gravity="center"
        android:text="新增退货地址"
        android:textColor="@android:color/white"
        android:textSize="16sp" />
</LinearLayout>

