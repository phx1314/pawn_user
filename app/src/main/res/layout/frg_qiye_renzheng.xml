                    </LinearLayout>
                    <ImageView android:layout_width="match_parent" android:layout_height="1px" android:background="#E2E2E2" />
                    <LinearLayout android:layout_width="match_parent" android:layout_height="wrap_content" android:gravity="center_vertical" android:orientation="horizontal">
                         <TextView style="@style/text_style_15_black" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="身份证号码" />
                         <EditText android:id="@+id/mEditText_sfz" style="@style/text_style_15_black" android:layout_width="match_parent" android:layout_height="wrap_content" android:background="#00000000" android:hint="请输入" android:padding="@dimen/j16dp" android:singleLine="true" />
                    </LinearLayout>
               </LinearLayout>
               <LinearLayout android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginTop="@dimen/j18dp" android:background="@drawable/shape_white_rz" android:orientation="vertical" android:padding="@dimen/j10dp">
                    <TextView style="@style/text_style_15_black" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="@dimen/j16dp" android:text="营业执照副本照片：" />
                    <LinearLayout android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginTop="@dimen/j24dp" android:orientation="horizontal">
                         <RelativeLayout android:layout_width="0dp" android:layout_height="wrap_content" android:layout_weight="1">
                              <ImageView android:id="@+id/mImageView_yyz" android:layout_width="match_parent" android:layout_height="@dimen/j96dp" android:src="@drawable/yyz" />
                              <ImageView android:id="@+id/mImageView_zmjia" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_centerInParent="true" android:src="@drawable/jiajia" />
                         </RelativeLayout>
                         <RelativeLayout android:layout_width="0dp" android:layout_height="wrap_content" android:layout_weight="1" />
                    </LinearLayout>
                    <ImageView android:layout_width="match_parent" android:layout_height="1px" android:layout_marginTop="@dimen/j32dp" android:background="#E2E2E2" />
                    <TextView style="@style/text_style_15_black" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="@dimen/j16dp" android:text="法人身份证正面/反面照：" />
